{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@app/*": ["src/app/*"],
      "@pages/*": ["src/pages/*"],
      "@firebase/*": ["src/Firebase/*"],
    }
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "static/**/*", "functions-src/**/*"]
}